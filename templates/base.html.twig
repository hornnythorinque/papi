<!doctype html>
<html lang="fr">
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			{% block title %}papi
			{% endblock %}
		</title>
		{% block stylesheets %}
			<link href="https://bootswatch.com/5/lux/bootstrap.min.css" rel="stylesheet">
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
		{% endblock %}
	</head>
	<body>
		<nav class="navbar navbar-expand-lg bg-primary" data-bs-theme="dark">
			<div class="container-fluid">
				<a class="navbar-brand" href="{{path('app_accueil')}}" share>home</a>
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse" id="navbarColor01">
					<ul class="navbar-nav me-auto">
						<li class="nav-item">
							<a class="nav-link active" href="{{path('app_accueil')}}">Home
								<span class="visually-hidden">(current)</span>
							</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="https://s4-8081.nuage-peda.fr/share/contact">Contact</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="https://s4-8081.nuage-peda.fr/share/about">à propos</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="https://s4-8081.nuage-peda.fr/share/liste-contacts">liste-contacts</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="https://s4-8081.nuage-peda.fr/share/login">login</a>
						</li>
						
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" ariaexpanded="false">Catégorie</a>
							<div class="dropdown-menu">
								<a class="nav-link" href="https://s4-8081.nuage-peda.fr/share/category">ajout</a>
								<a class="nav-link" href="https://s4-8081.nuage-peda.fr/share/liste-categorys">liste</a>
							</div>
						</li>

					</ul>
					<form class="d-flex">
						<input class="form-control me-sm-2" type="search" placeholder="Search">
						<button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
					</form>
				</div>
			</div>
		</nav>
		{% for message in app.flashes('notice') %}
			<h2 class="alert alert-warning text-center mt-4 mb-4" role="alert">
				{{ message }}
			</h2>
		{% endfor %}

		{% block body %}Share
		{% endblock %}
		{% block javascripts %}
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" ></script>
		{% endblock %}
	</body>
</html>
